\n  componentWillUnmount() {\n    this.unsubscribeFromAuth();\n  }\n\n  render() {\n    return (\n      <div>\n        <Header/>\n        <Switch>\n          <Route exact path='/' component={HomePage} />\n          <Route path='/shop' component={ShopPage} />\n          <Route path='/signin' component={SignInAndSignUpPage}/>\n        </Switch>\n      </div>\n    );\n  }\n}\n\nconst mapDispatchToProps = dispatch => ({\n  setCurrentUser: user => dispatch(setCurrentUser(user))\n});\n\nexport default connect(null, mapDispatchToProps)(App);\n","/home/leo/code/leonardo-vanschaik/react-ecommerce-site/src/reportWebVitals.js",[],["90","91"],"/home/leo/code/leonardo-vanschaik/react-ecommerce-site/src/pages/homepage/homepage.component.jsx",[],["92","93"],"/home/leo/code/leonardo-vanschaik/react-ecommerce-site/src/components/directory/directory.component.jsx",[],"/home/leo/code/leonardo-vanschaik/react-ecommerce-site/src/components/menu-item/menu-item.component.jsx",[],"/home/leo/code/leonardo-vanschaik/react-ecommerce-site/src/pages/shop/shop.component.jsx",[],"/home/leo/code/leonardo-vanschaik/react-ecommerce-site/src/pages/shop/shop.data.js",[],"/home/leo/code/leonardo-vanschaik/react-ecommerce-site/src/components/collection-preview/collection-preview.component.jsx",[],"/home/leo/code/leonardo-vanschaik/react-ecommerce-site/src/components/collection-item/collection-item.component.jsx",[],"/home/leo/code/leonardo-vanschaik/react-ecommerce-site/src/components/header/header.component.jsx",[],"/home/leo/code/leonardo-vanschaik/react-ecommerce-site/src/pages/sign-in-and-sign-up/sign-in-and-sign-up.component.jsx",[],"/home/leo/code/leonardo-vanschaik/react-ecommerce-site/src/components/sign-in/sign-in.component.jsx",[],"/home/leo/code/leonardo-vanschaik/react-ecommerce-site/src/components/form-input/form-input.component.jsx",[],"/home/leo/code/leonardo-vanschaik/react-ecommerce-site/src/components/custom-button/custom-button.component.jsx",[],"/home/leo/code/leonardo-vanschaik/reac